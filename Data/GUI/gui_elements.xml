<?xml version="1.0" encoding="ISO-8859-1"?>
<gui_elements>

	<gui_spritemap name="gui1" width="1024" height="1024">
		<material name="gui1" renderable_object_technique="MV_POSITION4_NORMAL_TEXTURE_VERTEX">
			<texture filename="Data\\GUI\\textures\\buttons.png"/>
		</material>
		<sprite name="buttonPlay_idle" x="45" y="196" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonPlay_over" x="386" y="196" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonPlay_pressed" x="710" y="196" w="255" h="90" pX="0.5" pY="0.5"/>
		
		<sprite name="buttonOptions_idle" x="45" y="342" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonOptions_over" x="386" y="342" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonOptions_pressed" x="710" y="342" w="255" h="90" pX="0.5" pY="0.5"/>
		
		<sprite name="buttonCredits_idle" x="45" y="467" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonCredits_over" x="386" y="467" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonCredits_pressed" x="710" y="467" w="255" h="90" pX="0.5" pY="0.5"/>
		
		<sprite name="buttonBack_idle" x="45" y="616" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonBack_over" x="386" y="616" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonBack_pressed" x="710" y="616" w="255" h="90" pX="0.5" pY="0.5"/>
		
		<sprite name="buttonExit_idle" x="45" y="763" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonExit_over" x="386" y="763" w="255" h="90" pX="0.5" pY="0.5"/>
		<sprite name="buttonExit_pressed" x="710" y="763" w="255" h="90" pX="0.5" pY="0.5"/>
	</gui_spritemap>

	<gui_spritemap name="guisliders" width="512" height="512">
		<material name="gui1" renderable_object_technique="MV_POSITION4_NORMAL_TEXTURE_VERTEX">
			<texture filename="Data\\GUI\\textures\\Sliders.png"/>
		</material>
		<sprite name="slider_base" x="26" y="50" w="425" h="43" pX="0.5" pY="0.5"/>
		<sprite name="slider_top" x="26" y="134" w="395" h="14" pX="0.5" pY="0.5"/>
		<sprite name="slider_handle" x="26" y="232" w="28" h="44" pX="0.5" pY="0.5"/>
		<sprite name="slider_pressed_handle" x="26" y="292" w="28" h="44" pX="0.5" pY="0.5"/>
	</gui_spritemap>
	
	<gui_spritemap name="fontTest" width="256" height="256" >
		<material name="fontTest" renderable_object_technique="MV_POSITION4_NORMAL_TEXTURE_VERTEX">
			<texture filename="Data\\Fonts\\fontTest_0.png"/>
		</material>
		
		<sprite name="fontTest_0.png" x="0" y="0" w="256" h="256" />
	</gui_spritemap>
	
	<button name="play_button" normal="buttonPlay_idle" highlight="buttonPlay_over" pressed="buttonPlay_pressed"/>
	<button name="options_button" normal="buttonOptions_idle" highlight="buttonOptions_over" pressed="buttonOptions_pressed"/>
	<button name="credits_button" normal="buttonCredits_idle" highlight="buttonCredits_over" pressed="buttonCredits_pressed"/>
	<button name="back_button" normal="buttonBack_idle" highlight="buttonBack_over" pressed="buttonBack_pressed"/>
	<button name="exit_button" normal="buttonExit_idle" highlight="buttonExit_over" pressed="buttonExit_pressed"/>
	<font name="fontTest" path="Data\\Fonts\\fontTest.fnt" />
	<slider name="volume_slider" base="slider_base" top="slider_top" handle="slider_handle" pressed_handle="slider_pressed_handle"/>
	
</gui_elements>
		