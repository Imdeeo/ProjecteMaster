<?xml version="1.0" encoding="ISO-8859-1"?>
<materials>
	<material name="SSRMaterial" renderable_object_technique="ssr">
		<parameter type="float" name="enabled" value="1.0"/>
		<parameter type="float" name="ssr_quality" value="1.5"/>
		<parameter type="float" name="ssr_opacity" value="0.3" description="min=0.0	max=1.0 step=0.05"/>
		<parameter type="float" name="ssr_increment_multiplier" value="1.3"	description="min=0.0 max=3.0 step=0.1"/>
		<parameter type="float" name="ssr_offset_screen" value="0.05" description="min=0.0 max=1.0 step=0.02"/>
		<parameter type="vect2f" name="screen_resolution" value="960 540"/>
	</material>
	<material name="NoiseAndVignettingMaterial" renderable_object_technique="noise_and_vignetting">
		<texture type="diffuse" filename="Data\GUI\Textures\noise.dds"/>
		<texture type="diffuse" filename="Data\GUI\Textures\vignetting.dds"/>
	</material>
	<material name="BlurMaterial" renderable_object_technique="blur">
		<parameter name ="texture_size" type="float" value="1280.0"/>
		<parameter name ="blur_scale" type="float" value="2.0"/>
	</material>
	<material name="GUIMaterial" renderable_object_technique="gui">
	</material>
	<material name="ZBlurMaterial" renderable_object_technique="zblur">
		<parameter name ="ZBlurActive" type="float" value="1.0"/>
		<parameter name ="ZBlurShowDepths" type="float" value="1.0"/>
		<parameter name ="ZBlurConstantBlur" type="float" value="0.0"/>
		<parameter name ="ZBlurFocalStart" type="float" value="1.0"/>
		<parameter name ="ZBlurFocalEnd" type="float" value="25.0"/>
		<parameter name ="ZBlurEnd" type="float" value="50.0"/>
	</material>
	<material name="BloomMaterial" renderable_object_technique="bloom">
		<parameter name ="Threshold" type="float" value="0.9"/>
	</material>
	<material name="BloomBlurMaterial" renderable_object_technique="bloom_blur">
		<parameter name ="TextureSize" type="float" value="1280.0"/>
		<parameter name ="BlurScale" type="float" value="2.0"/>
	</material>
	<material name="BloomCombineMaterial" renderable_object_technique="bloom_combine">
		<parameter name ="m_BloomActive" type="float" value="1.0"/>
		<parameter name ="m_BloomIntensity" type="float" value="0.001"/>
		<parameter name ="m_OriginalIntensity" type="float" value="1.0"/>
		<parameter name ="m_BloomSaturation" type="float" value="2.0"/>
		<parameter name ="m_OriginalSaturation" type="float" value="1.5"/>
	</material>
	<material name="HDAOMaterial" renderable_object_technique="hdao">		
		<parameter type="float" name="HDAOActive" value="1.0" description="min=0.0 max=1.0 step=1.0" />
		<parameter type="float" name="HDAOShowNormals" value="0.0" description="min=0.0 max=1.0 step=1.0"/>
		<parameter type="float" name="HDAOShowAO" value="0.0" description="min=0.0 max=1.0 step=1.0" />
		<parameter type="float" name="HDAOUseNormals" value="1.0" description="min=0.0 max=1.0 step=1.0" />
		<parameter type="float2" name="RTSize" value="1280.0 720.0" description="" />
		<parameter type="float" name="AcceptAngle" value="0.5" description="min=0.0 max=1.5 step=0.001" />
		<parameter type="float" name="HDAOIntensity" value="2.14" description="min=0.0 max=5.0 step=0.05" />
		<parameter type="float" name="NormalScale" value="0.4" description="min=0.0 max=1.0 step=0.001" />
		<parameter type="float" name="HDAORejectRadius" value="0.43" description="min=0.0 max=1.0 step=0.001" />
		<parameter type="float" name="HDAOAcceptRadius" value="0.00312" description="min=0.0 max=1.0 step=0.001" />
	</material>
	<material name="ColorGradingMaterial" renderable_object_technique="colorgrading">
		<parameter name ="ColorGradingActive" type="float" value="1.0"/>
		<parameter name ="BrownSepiaColorGrading" type="float" value="0.1"/>
		<parameter name ="GreenSepiaColorGrading" type="float" value="0.1"/>
		<parameter name ="BlueSepiaColorGrading" type="float" value="1.0"/>
		<parameter name ="MonochromeColorGrading" type="float" value="0.5"/>
		<parameter name ="ColorColorGrading" type="float" value="1.0"/>
		<parameter name ="ContrastColorGrading" type="float" value="0.4"/>
		<parameter name ="BrightnessColorGrading" type="float" value="0.05"/>
	</material>
	<material name="FogMaterial" renderable_object_technique="fog">
		<parameter name ="MaxAttenuation_StartLinearFog_EndLinearFog" type="float3" value="0.5 50 100"/>
		<parameter name ="FogColor" type="float3" value="1 1 1"/>
	</material>
</materials>
